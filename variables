1)package main

import (
	"fmt"
)

func main() {
	fmt.Println("Hello kloudne")
}

Hello kloudne

Program exited.


2)package main

import "fmt"

func main() {
   var x float64
   x = 200.000
   fmt.Println(x)
   fmt.Printf("x is of type %T\n", x)
}


200
x is of type float64

Program exited.

3)package main

import "fmt"

func main() {
   var x float64 = 20.0
   var y float64 = 25.00 
   fmt.Println(x)
   fmt.Println(y)
   fmt.Printf("x is of type %T\n", x)
   fmt.Printf("y is of type %T\n", y)	
}


20
25
x is of type float64
y is of type float64

Program exited.
4)package main

import "fmt"

func main() {
   var x float64 = 20.0

   y := 42
   fmt.Println(x)
   fmt.Println(y)
   fmt.Printf("x is of type %T\n", x)
   fmt.Printf("y is of type %T\n", y)	
}
20
42
x is of type float64
y is of type float64

Program exited.

5)package main

import "fmt"

func main() {
   var a, b, c = 3, 4, "kloud one"  
	
   fmt.Println(a)
   fmt.Println(b)
   fmt.Println(c)
   fmt.Printf("a is of type %T\n", a)
   fmt.Printf("b is of type %T\n", b)
   fmt.Printf("c is of type %T\n", c)
}

3
4
kloud one
a is of type int
b is of type int
c is of type string

Program exited.



6)
package main

import "fmt"

func main() {

	var a = "kloudone"
	fmt.Println(a)

	
	var b, c int = 1, 2
	fmt.Println(b, c)

	
	var d = true
	fmt.Println(d)

	
	var e int
	fmt.Println(e)

	
	f := "chennai"
	fmt.Println(f)
}

kloudone
1 2
true
0
chennai

Program exited.

7)package main

import "fmt"

var c, python, java bool

func main() {
	var i int
	fmt.Println(i, c, python, java)
}

0 false false false

Program exited.








